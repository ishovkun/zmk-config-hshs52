#include <physical_layouts.dtsi>

/ {
    hshs52_layout: hshs52_layout {
        compatible = "zmk,physical-layout";
        display-name = "HSHS52 Layout";
        transform = <&default_transform>;
        keys  //                     w   h    x    y     rot    rx    ry
            = <&key_physical_attrs 100 100    0    0       0     0     0>   // ` key
            , <&key_physical_attrs 100 100  100    0       0     0     0>   // Q
            , <&key_physical_attrs 100 100  200   (-20)    0     0     0>   // W
            , <&key_physical_attrs 100 100  300   (-40)    0     0     0>   // E
            , <&key_physical_attrs 100 100  400   (-20)    0     0     0>   // R
            , <&key_physical_attrs 100 100  500    0       0     0     0>   // T
            , <&key_physical_attrs 100 100  800    0       0     0     0>   // Y
            , <&key_physical_attrs 100 100  900   (-20)    0     0     0>   // U
            , <&key_physical_attrs 100 100 1000   (-40)    0     0     0>   // I
            , <&key_physical_attrs 100 100 1100   (-20)    0     0     0>   // O
            , <&key_physical_attrs 100 100 1200    0       0     0     0>   // P
            , <&key_physical_attrs 100 100 1300    0       0     0     0>   // DEL

            , <&key_physical_attrs 100 100    0  100       0     0     0>   // TAB
            , <&key_physical_attrs 100 100  100  100       0     0     0>   // A
            , <&key_physical_attrs 100 100  200   80       0     0     0>   // S
            , <&key_physical_attrs 100 100  300   60       0     0     0>   // D
            , <&key_physical_attrs 100 100  400   80       0     0     0>   // F
            , <&key_physical_attrs 100 100  500  100       0     0     0>   // G
            , <&key_physical_attrs 100 100  800  100       0     0     0>   // H
            , <&key_physical_attrs 100 100  900   80       0     0     0>   // J
            , <&key_physical_attrs 100 100 1000   60       0     0     0>   // K
            , <&key_physical_attrs 100 100 1100   80       0     0     0>   // L
            , <&key_physical_attrs 100 100 1200  100       0     0     0>   // ;
            , <&key_physical_attrs 100 100 1300  100       0     0     0>   // '

            , <&key_physical_attrs 100 100    0  200       0     0     0>   // SHIFT/(
            , <&key_physical_attrs 100 100  100  200       0     0     0>   // Z
            , <&key_physical_attrs 100 100  200  180       0     0     0>   // X
            , <&key_physical_attrs 100 100  300  160       0     0     0>   // C
            , <&key_physical_attrs 100 100  400  180       0     0     0>   // V
            , <&key_physical_attrs 100 100  500  200       0     0     0>   // B
            , <&key_physical_attrs 100 100  600  200       0     0     0>   // RET
            // right half
            , <&key_physical_attrs 100 100  700  200       0     0     0>   // RET
            , <&key_physical_attrs 100 100  800  200       0     0     0>   // N
            , <&key_physical_attrs 100 100  900  180       0     0     0>   // M
            , <&key_physical_attrs 100 100 1000  160       0     0     0>   // ,
            , <&key_physical_attrs 100 100 1100  180       0     0     0>   // .
            , <&key_physical_attrs 100 100 1200  200       0     0     0>   // /
            , <&key_physical_attrs 100 100 1300  200       0     0     0>   // RSHIFT
            //
            // , <&key_physical_attrs 100 100  800  200       0     0     0>   // N
            // , <&key_physical_attrs 100 100  900  180       0     0     0>   // M
            // , <&key_physical_attrs 100 100 1000  160       0     0     0>   // ,
            // , <&key_physical_attrs 100 100 1100  180       0     0     0>   // .
            // , <&key_physical_attrs 100 100 1200  200       0     0     0>   // /
            // , <&key_physical_attrs 100 100 1300  200       0     0     0>   // SHFT/RET
            //

            // , <&key_physical_attrs 100 100    0  300       0     0     0>   // SHIFT/(
            // , <&key_physical_attrs 100 100  100  300       0     0     0>   // ESC
            , <&key_physical_attrs 100 100    0  300       0     0     0>   // CTRL
            , <&key_physical_attrs 100 100  100  300       0     0     0>   // SYM
            , <&key_physical_attrs 100 100  200  300       0     0     0>   // GUI
            , <&key_physical_attrs 100 100  300  300       0     0     0>   // Ctl/ESC
            , <&key_physical_attrs 100 100  400  300       0     0     0>   // Nav/SPC
            , <&key_physical_attrs 100 100  500  300       0     0     0>   // Nav/SPC
            , <&key_physical_attrs 100 100  600  300       0     0     0>   // Ctl/ESC
            , <&key_physical_attrs 100 100  700  300       0     0     0>   // Sym/SPC
            , <&key_physical_attrs 100 100  800  300       0     0     0>   // SYM
            , <&key_physical_attrs 100 100  900  300       0     0     0>   // backslash
            , <&key_physical_attrs 100 100 1000  300       0     0     0>   // [
            , <&key_physical_attrs 100 100 1100  300       0     0     0>   // ]
            ;
    };
};
